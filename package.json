{
  "name": "keep-it-secret",
  "version": "1.0.0",
  "description": "secret social media",
  "main": "index.js",
  "scripts": {
    "start": "npm run serve",
    "build": "npm run build-ts && npm run configs && npm run tslint",
    "serve": "node dist/index.js",
    "watch-node": "nodemon dist/index.js",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "start:debug": "node -r ts-node/register --inspect-brk=5858 src/index.ts",
    "watch:debug": "nodemon -e ts,tsx --exec npm run start:debug",
    "serve-debug": "nodemon --inspect dist/index.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "configs": "copyfiles -u 1 src/configurations/*.json dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ToushiShu/keep-it-secret.git"
  },
  "keywords": [
    "social",
    "media",
    "secret",
    "encryption"
  ],
  "author": "Toushishu",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ToushiShu/keep-it-secret/issues"
  },
  "homepage": "https://github.com/ToushiShu/keep-it-secret#readme",
  "dependencies": {
    "@types/compression": "^0.0.36",
    "@types/express": "^4.16.1",
    "@types/mongoose": "^5.3.24",
    "@types/nconf": "^0.10.0",
    "body-parser": "^1.18.3",
    "compression": "^1.7.4",
    "express": "^4.16.4",
    "express-validator": "^5.3.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.4.22",
    "morgan": "^1.9.1",
    "nconf": "^0.10.0",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "copyfiles": "^2.1.0",
    "cross-env": "^5.2.0",
    "mocha": "^6.0.2",
    "nodemon": "^1.18.10",
    "ts-node": "^8.0.3",
    "tslint": "^5.15.0",
    "typescript": "^3.4.2"
  }
}
